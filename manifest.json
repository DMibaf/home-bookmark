{

  "manifest_version": 2,
  "name": "HomeBookmark",
  "version": "0.1",
  "description": "New tab replacement",
  "homepage_url": "https://github.com/DMibaf/home-bookmark",
  "author": "Mibaf",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "icons": {
    "48": "assets/icon48.png",
		"96": "assets/icon96.png"
  }
}
